<t tx="ekr.20031218072017.3783">def canonicalizeMenuName (self,name):
    
    name = name.lower() ; newname = ""
    chars = string.ascii_letters + string.digits
    for ch in name:
        # if ch not in (' ','\t','\n','\r','&amp;'):
        if ch in chars:
            newname = newname+ch
    return newname
    
def canonicalizeTranslatedMenuName (self,name):
    
    name = name.lower() ; newname = ""
    for ch in name:
