    if sys.platform == "darwin":
        if ctrl and not cmd:
            cmd = True ; ctrl = False
        if alt and not ctrl:
            ctrl = True ; alt = False
    if g.app.new_keys: # New, simplified.
        &lt;&lt; convert minus signs to plus signs &gt;&gt;
        &lt;&lt; compute the last field &gt;&gt;
        &lt;&lt; compute shortcut &gt;&gt;
