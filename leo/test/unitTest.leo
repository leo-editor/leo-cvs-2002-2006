<t tx="ekr.20050106133147">def removeLeadingWs (code):

    if not code.strip():
        return ''

    lines = g.splitLines(code)
    line = lines[0]
    i = g.skip_ws(line,0)
    ws = line[0:i]
    if not ws:
        return code
    result = [] ; n = len(ws)
    for line in lines:
        if line.startswith(ws):
            result.append(line[n:])
        else:
            print 'underindented line:%s' % repr(line)
            result.append(line)
            
    # g.trace(g.listToString(result))

    result = ''.join(result)
    return result</t>
