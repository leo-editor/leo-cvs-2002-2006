    body = '''&lt;?xml version="1.0"?&gt;
&lt;xsl:transform xmlns:xsl="http:///www.w3.org/1999/XSL/Transform" version="1.0"&gt;    
&lt;/xsl:transform&gt;'''

    tnode = leoNodes.tnode(body,"xslt stylesheet")
    c.beginUpdate()
    try:
        pos.insertAfter(tnode)
    finally:
        c.endUpdate()
