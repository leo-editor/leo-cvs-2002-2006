<t tx="ekr.20040303062846.22">def __init__ (self,c,parent,before,after,sel,ins,temp_v):
	
	# Init the base class.
	unittest.TestCase.__init__(self)

	self.c = c
	self.parent = parent
	self.before = before
	self.after  = after
	self.sel    = sel # Two lines giving the selection range in tk coordinates.
