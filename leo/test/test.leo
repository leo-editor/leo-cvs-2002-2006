<t tx="ekr.20040712101754.264">def makePluginsSuite(verbose=False,*args,**keys):
    
    """Create an plugins test for every .py file in the plugins directory."""
    
    plugins_path = g.os_path_join(g.app.loadDir,"..","plugins")
    
