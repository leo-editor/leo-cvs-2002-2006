    outfile.close()
    
    # get the sources back.
    sentinel.main(self.configfilename, "pull")
    
    # re-generate the output.
    sentinel.main(self.configfilename, "push")
    
    # and check for equality.
    assert file(self.output_filename).readlines() == lines
    
    
    
    
    
    



